#these are ordered top-down according to inter-dependencies 
find_library(JACKTELOP-GUI_LIBRARY NAMES jacktelop-gui HINTS @CMAKE_INSTALL_PREFIX@/lib)
find_library(JACKTELOP-LISTENER_LIBRARY NAMES jacktelop-listener HINTS @CMAKE_INSTALL_PREFIX@/lib)

set(JACKTELOP_LIBRARIES ${JACKTELOP_GUI_LIBRARY} ${JACKTELOP_LISTENER_LIBRARY})
mark_as_advanced(JACKTELOP_INCLUDE_DIR JACKTELOP_LIBRARIES)
include(FindPackageHandleStandardArgs) 
FIND_PACKAGE_HANDLE_STANDARD_ARGS(jacktelop FOUND_VAR jacktelop_FOUND REQUIRED_VARS JACKTELOP_LIBRARIES JACKTELOP_INCLUDE_DIR)
